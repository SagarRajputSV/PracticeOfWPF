<Window x:Class="Practice.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Practice"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Canvas Background="Aqua">
            <Rectangle Canvas.Left="10" Canvas.Top="10" Height="200" Width="200" Stroke="Black" StrokeThickness="5" Fill="OrangeRed"/>
            <Rectangle Canvas.Left="50" Canvas.Top="50" Height="200" Width="200" Stroke="Blue" StrokeThickness="5" Fill="White"></Rectangle>
            <Rectangle Canvas.Left="90" Canvas.Top="90" Height="200" Width="200" Stroke="Black" StrokeThickness="5" Fill="Green"></Rectangle>
        </Canvas>
    </Grid>
</Window>
