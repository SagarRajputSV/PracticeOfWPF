<Window x:Class="Practice.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Practice"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <DockPanel Name="dcPanel">
            <Button Name="Top" Height="50" DockPanel.Dock="Top" Content="Top" />
            <!--<Button Name="Bottom" Height="50" DockPanel.Dock="Bottom" Content="Bottom"/>-->
            <!--Uncomment the above line to check the difference-->
            <Button Name="Left" Width="50" DockPanel.Dock="Left" Content="Left"/>
            <Button Name="Right" Width="50" DockPanel.Dock="Right" Content=" Right"/>
            <Button Name="Bottom" Height="50" DockPanel.Dock="Bottom" Content="Bottom"/>

            <Button Name="Center" Content="Fill"/>

        </DockPanel>
            
    </Grid>
</Window>
